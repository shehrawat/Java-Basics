package selenium;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Webdriver_Chromedriver 
{

	public static void main(String[] args)
	{
		System.setProperty("webdriver.chrome.driver","D:\\JAVA\\Seleniom Project 3\\Selleniom\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://www.facebook.com/");
		driver.findElement(By.id("email")).sendKeys("ajayseherawat1@gmail.com");
	    driver.findElement(By.id("pass")).sendKeys("admin1234");
	    driver.findElement(By.id("u_0_2")).click();
		
	}

}
